Source: "F:\Jay Jawalkar\Flutter Apps\Flutter_Apps\exam_block\build\windows\x64\runner\Release\exam_block.exp"; DestDir: "{app}"; Flags: ignoreversion
Source: "F:\Jay Jawalkar\Flutter Apps\Flutter_Apps\exam_block\build\windows\x64\runner\Release\exam_block.lib"; DestDir: "{app}"; Flags: ignoreversion
Source: "F:\Jay Jawalkar\Flutter Apps\Flutter_Apps\exam_block\build\windows\x64\runner\Release\firebase_auth_plugin.lib"; DestDir: "{app}"; Flags: ignoreversion
Source: "F:\Jay Jawalkar\Flutter Apps\Flutter_Apps\exam_block\build\windows\x64\runner\Release\firebase_core_plugin.lib"; DestDir: "{app}"; Flags: ignoreversion
Source: "F:\Jay Jawalkar\Flutter Apps\Flutter_Apps\exam_block\build\windows\x64\runner\Release\flutter_windows.dll"; DestDir: "{app}"; Flags: ignoreversion
